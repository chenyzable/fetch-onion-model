{
  "name": "@cyzeal/fetch-onion-model",
  "version": "0.0.3",
  "description": "一个基于 Web Fetch Api 的轻量级 Http 请求增强工具，支持浏览器与 Node 环境，类似于 koa 的洋葱模型，以中间件形式扩展功能。",
  "keywords": [
    "fetch",
    "ajax",
    "request"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "homepage": "https://github.com/chenyzable/fetch-onion-model#readme",
  "author": "chenyzable <chenyz811@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --max-warnings=0",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "rimraf esm lib && rollup -c rollup.config.js",
    "release": "release-it"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js",
      "types": "./esm/index.d.ts"
    },
    "./esm/middleware/*": "./esm/middleware/*",
    "./lib/middleware/*": "./lib/middleware/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "esm"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "fetch-retry": "^5.0.6"
  },
  "devDependencies": {
    "@cyzeal/lint-config-lib": "^0.0.6",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@typescript-eslint/typescript-estree": "^6.7.3",
    "@vitest/coverage-v8": "^0.34.5",
    "abortcontroller-polyfill": "^1.7.5",
    "fast-glob": "^3.3.1",
    "msw": "^1.3.1",
    "node-fetch": "^3.3.2",
    "release-it": "^16.2.1",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.3",
    "rollup-plugin-dts": "^6.0.2",
    "typescript": "~5.1.2",
    "vitest": "^0.34.5"
  }
}
